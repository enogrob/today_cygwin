[user]
  name = Roberto Nogueira
  email = enogrob@gmail.com
  
[push]
  default = simple
  
[diff]
  tool = examdiff
  guitool = examdiff
  
[difftool "examdiff"]
  cmd = examdiff \"$(cygpath -wla $LOCAL)\" \"$(cygpath -wla $REMOTE)\" /nh
  
[difftool]
  prompt = false
  
[color]
  diff = auto
  status = auto
  branch = auto
  interactive = auto
  ui = true
  pager = true
  
[color "status"]
  added = green
  changed = red bold
  untracked = magenta bold

[color "branch"]
  remote = yellow

  